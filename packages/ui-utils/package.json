{
  "name": "@graviola/edb-ui-utils",
  "version": "0.2.6",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "CHANGELOG.md",
    "README.md"
  ],
  "license": "MIT",
  "scripts": {
    "depcheck": "depcheck",
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/**/*.ts",
    "lint-fix": "eslint --fix src/**/*.ts",
    "pack-clean": "bun run clean-package && (bun pm pack || true) ; bun run clean-package restore",
    "publish-clean": "bun run clean-package && npm publish --access public && bun run clean-package restore",
    "test": "jest",
    "test:watch": "jest --watch --color",
    "test:coverage": "jest --coverage"
  },
  "devDependencies": {
    "@graviola/edb-tsconfig": "workspace:*",
    "@graviola/edb-tsup-config": "workspace:*",
    "@types/lodash-es": "^4.17.12",
    "ajv": "^8.12",
    "ajv-formats": "^3.0.1",
    "eslint-config-edb": "workspace:*",
    "typescript": "^5.8.2",
    "tsup": "^8.0.0",
    "dayjs": "^1.11.11",
    "react": "^18"
  },
  "peerDependencies": {
    "@jsonforms/core": "^3",
    "@jsonforms/material-renderers": "^3",
    "@jsonforms/react": "^3",
    "@mui/icons-material": "^5",
    "@mui/material": "^5",
    "dayjs": "^1",
    "ajv": "^8.12",
    "ajv-formats": "^3",
    "react": "^18"
  },
  "dependencies": {
    "@graviola/edb-core-utils": "workspace:*",
    "json-schema-to-ts": "^3.0.1",
    "lodash-es": "^4.17.21"
  },
  "clean-package": {
    "extends": "../../../clean-package.config.cjs"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "eslint-config-edb"
    ]
  }
}
