{
  "name": "@slub/exhibition-sparql-config",
  "description": "Semantic Namespace and SPARQL endpoint configuration for the SLUB exhibitions project",
  "version": "1.2.5",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
